{
	"name": "tradeverse",
	"version": "0.0.1",
	"type": "module",
	"description": "A discord bot for tradeverse server",
	"private": true,
	"license": "UNLICENSED",
	"imports": {
		"#zod": "./dist/src/core/zod/",
		"#mongoose": "./dist/src/core/mongo/",
		"#mongoose/*": "./dist/src/core/mongo/*",
		"#core": "./dist/src/core/",
		"#core/*": "./dist/src/core/*",
		"#utils": "./dist/src/utils/",
		"#utils/*": "./dist/src/utils/*",
		"#types": "./dist/src/types/",
		"#types/*": "./dist/src/types/*",
		"#components": "./dist/src/components/",
		"#components/*": "./dist/src/components/*",
		"#consumers": "./dist/src/consumers/",
		"#consumers/*": "./dist/src/consumers/*",
		"#services": "./dist/src/services/",
		"#services/*": "./dist/src/services/*",
		"#producers": "./dist/src/producers/",
		"#producers/*": "./dist/src/producers/*"
	},
	"scripts": {
		"dev": "nest start --watch",
		"build": "nest build",
		"start": "node --enable-source-maps dist/main.js",
		"lint": "TIMING=1 eslint \"{src,tests}/**/*.ts\" --fix",
		"format": "prettier --write --cache .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
		"test:e2e": "vitest run --config ./vitest.config.e2e.ts",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"sync": "bun ./src/core/mongo/schemaToZod.ts"
	},
	"dependencies": {
		"@discordjs/builders": "^1.10.1",
		"@discordjs/rest": "^2.4.3",
		"@fastify/static": "^8.1.1",
		"@golevelup/nestjs-rabbitmq": "5.7.0",
		"@nestjs/common": "^11.0.10",
		"@nestjs/config": "^4.0.0",
		"@nestjs/core": "^11.0.10",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/mongoose": "^11.0.1",
		"@nestjs/platform-fastify": "^11.0.10",
		"@nestjs/serve-static": "^5.0.2",
		"@nestjs/swagger": "^11.0.3",
		"@sapphire/snowflake": "^3.5.5",
		"@swc/helpers": "^0.5.15",
		"amqp-connection-manager": "4.1.14",
		"amqplib": "0.10.5",
		"discord.js": "^14.18.0",
		"dotenv": "^16.4.7",
		"ejs": "^3.1.10",
		"fastify": "^5.2.1",
		"helmet": "^8.0.0",
		"mongoose": "^8.10.1",
		"oxide.ts": "^1.1.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1",
		"undici": "^7.3.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@darraghor/eslint-plugin-nestjs-typed": "^6.2.4",
		"@nestjs/cli": "^11.0.4",
		"@nestjs/schematics": "^11.0.1",
		"@nestjs/testing": "^11.0.10",
		"@swc/cli": "^0.6.0",
		"@swc/core": "^1.10.17",
		"@types/node": "^22.13.4",
		"@vitest/coverage-v8": "3.0.6",
		"eslint": "^9.20.1",
		"eslint-config-mahir": "^1.0.7",
		"json-schema-to-zod": "patch:json-schema-to-zod@npm%3A2.6.0#~/.yarn/patches/json-schema-to-zod-npm-2.6.0-7abe5270fe.patch",
		"mongoose-schema-jsonschema": "patch:mongoose-schema-jsonschema@npm%3A3.0.2#~/.yarn/patches/mongoose-schema-jsonschema-npm-3.0.2-7b2d1c042e.patch",
		"prettier": "^3.5.1",
		"typescript": "^5.7.3",
		"unplugin-swc": "^1.5.1",
		"vitest": "3.0.6"
	},
	"packageManager": "yarn@4.6.0"
}
